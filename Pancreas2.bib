	keywords = {Adenocarcinoma, Adenocarcinoma, Celiac Artery, Celiac Artery, Contrast Media, Contrast Media, Humans, Humans, Image Processing, Computer-Assisted, Image Processing, Computer-Assisted, Mesenteric Arteries, Mesenteric Arteries, Pancreas, Pancreas, Pancreatic Neoplasms, Pancreatic Neoplasms, Radiographic Image Enhancement, Radiographic Image Enhancement, Tomography, X-Ray Computed, Tomography, X-Ray Computed},
	pages = {214--221},
	file = {Balachandran et al. - 2008 - Arterial variants in pancreatic adenocarcinoma.pdf:/home/denest/zoterolib/storage/XACEK72U/Balachandran et al. - 2008 - Arterial variants in pancreatic adenocarcinoma.pdf:application/pdf}
