@article{__2013-3,
	title = {Значение онкомаркера са 19-9 в диагностике хронического панкреатита},
	issn = {2306-4145},
	abstract = {Исследование онкомаркера СА 19-9, являющееся простым и общедоступным методом диагностики рака поджелудочной железы, достаточно распространено в клинической практике. Исследованы результаты лечения и показатели онкомаркера СА 19-9 до и после операции у 112 больных хроническим панкреатитом и у 12 пациентовс аденокарциномой поджелудочной железы. У больных хроническим панкреатитом после устранения механической желтухи наблюдали статистически достоверное снижение онкомаркера СА 19-9 в отдаленном периоде. Показатели онкомаркера СА 19-9 у больных с аденокарциномой поджелудочной железы были достоверно выше, чем у пациентов хроническим панкреатитом., Determination of oncomarker {CA} 19-9 is simple and widely used diagnostic method for pancreatic cancer diagnostics. It is spread well enough in clinical practice. The article deals with the results of surgery in relation to determination of the oncomarker {CA} 19-9 before and after surgery in 112 chronic pancreatitis patients and 12 pancreatic adenocarcinoma patients. Chronic pancreatitis patients with mechanical jaundice have statistically proven decrease of the oncomarker {CA} 19-9 postoperatively. Measurement of the oncomarker {CA} 19-9 in pancreatic adenocarcinoma patients was statistically higher than in chronic pancreatitis patients.},
	language = {русский},
	number = {1 (76)},
	journal = {ЗАПОРОЖСКИЙ МЕДИЦИНСКИЙ ЖУРНАЛ},
	author = {КЛИМЕНКО, А. В.},
	year = {2013},
	keywords = {{CHRONIC} {PANCREATITIS}, {MECHANICAL} {JAUNDICE}, {ONCOMARKER} СА 19-9, {PANCREATICOJEJUNODUODENOSTOMY}, МЕХАНИЧЕСКАЯ ЖЕЛТУХА, МЕХАНіЧНА ЖОВТЯНИЦЯ, ОНКОМАРКЕР СА 19-9, ПАНКРЕАТОЕЮНОДУОДЕНОСТОМИЯ, ПАНКРЕАТОєЮНОДУОДЕНОСТОМіЯ, ХРОНИЧЕСКИЙ ПАНКРЕАТИТ, ХРОНіЧНИЙ ПАНКРЕАТИТ},
	pages = {029--031},
	file = {62357766.pdf:/home/denest/zoterolib/storage/N328EABN/62357766.pdf:application/pdf}
}

