	keywords = {Adult, Aged, Aged, 80 and over, Angiography, Carcinoma, Pancreatic Ductal, Female, Humans, Kaplan-Meier Estimate, Male, Mesenteric Artery, Superior, Middle Aged, Muscle, Smooth, Vascular, Neoplasm Grading, Neoplasm Invasiveness, Pancreatectomy, Pancreatic Neoplasms, Phlebography, Portal Vein, Prognosis, Retrospective Studies, Statistics as Topic},
	pages = {103--108}
