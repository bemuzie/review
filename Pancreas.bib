@article{ichikawa_duct-penetrating_2001,
	title = {Duct-penetrating Sign at {MRCP:} Usefulness for Differentiating Inflammatory Pancreatic Mass from Pancreatic Carcinomas1},
	volume = {221},
	issn = {0033-8419, 1527-1315},
	shorttitle = {Duct-penetrating Sign at {MRCP}},
	url = {http://radiology.rsna.org/content/221/1/107},
	doi = {10.1148/radiol.2211001157},
	abstract = {{PURPOSE:} To define the duct-penetrating sign at magnetic resonance ({MR)} cholangiopancreatography ({MRCP)} and to assess the usefulness of this sign for distinguishing an inflammatory pancreatic mass ({IPM)} from a conventional pancreatic carcinoma ({CPC)} compared with arterial phase computed tomography (hereafter, {CT)} and arterial phase {MR} imaging (hereafter, {MR} imaging).
{MATERIALS} {AND} {METHODS:} {MRCP}, {CT}, and {MR} images were compared by means of receiver operating characteristic ({ROC)} analysis for 11 {IPMs} and 43 {CPCs.} With the {MRCP} images, a morphologic classification of the main pancreatic duct ({MPD)} was attempted for all lesions. On the basis of this classification and the enhancement patterns of a lesion, all readers graded the presence of {IPM} or {CPC} on a five-point scale for all images.
{RESULTS:} On the {MRCP} images, the morphologic characteristics of the {MPD} were nonobstruction for {IPM} (28 of 33, 85\%) and obstruction or irregular stenosis for {CPC} (124 of 129, 96\%). At {ROC} analysis among all the techniques, {MRCP} images had the highest value (0.98) for significant areas under the {ROC} curve ({CT}, 0.84; {MR}, 0.76) (P {\textless} .001). For the duct-penetrating sign in the broad sense (nonobstructed {MPD)} and the sign in the narrow sense (only normal {MPD)}, the sensitivity, specificity, and accuracy for diagnosis of {IPM} were 85\%, 96\%, and 94\%, respectively, and 36\%, 100\%, and 87\%, respectively.
{CONCLUSION:} The duct-penetrating sign on {MRCP} images was more helpful to distinguish {IPM} from {CPC} than were the enhancement patterns on {CT} and {MR} images.},
	number = {1},
	urldate = {2012-02-28},
	journal = {Radiology},
	author = {Ichikawa, Tomoaki and Sou, Hironobu and Araki, Tsutomu and Arbab, Ali S and Yoshikawa, Takeharu and Ishigame, Keiichi and Haradome, Hiroki and Hachiya, Junichi},
	month = oct,
	year = {2001},
	keywords = {Pancreas, {CT}, 770.12113, 770.12115, Pancreas, {MR}, 770.121411, 770.121412, 77.121415, 770.12143, Pancreas, neoplasms, 774.321, Pancreatitis, 770.291},
	pages = {107--116},
	file = {Ichikawa et al. - 2001 - Duct-penetrating Sign at MRCP Usefulness for Diff.pdf:/home/denest/zoterolib/storage/7NFICZ5H/Ichikawa et al. - 2001 - Duct-penetrating Sign at MRCP Usefulness for Diff.pdf:application/pdf;Snapshot:/home/denest/zoterolib/storage/PJPP2UT5/107.html:text/html}
}

@article{mortele_multimodality_2006,
	title = {Multimodality Imaging of Pancreatic and Biliary Congenital Anomalies1},
	volume = {26},
	url = {http://radiographics.rsna.org/content/26/3/715.abstract},
	doi = {10.1148/rg.263055164},
	abstract = {A wide spectrum of anomalies of the pancreas, the pancreatic ductal system, and the biliary tree are commonly encountered at radiologic evaluation. These anomalies may simulate various neoplastic, inflammatory, and posttraumatic conditions and should be part of the differential diagnosis for a variety of abnormalities found at diagnostic imaging. Anatomic variants, developmental anomalies (eg, pancreas divisum, annular pancreas, ectopic pancreas, pancreatic agenesis and hypoplasia), and congenital diseases (congenital pancreatic cysts, von {Hippel–Lindau} disease, choledochal cysts), in addition to potential imaging pitfalls (uneven distribution of fat, “pseudomasses”), can all pose a diagnostic challenge for the radiologist. Familiarity with these anomalies, the imaging techniques available for their study, and their variable imaging manifestations is necessary for differentiating them from other biliary and pancreatic conditions. A basic understanding of the embryologic development and normal anatomy of the pancreas and biliary tree is also essential for identifying these anomalies.© {RSNA}, 2006},
	number = {3},
	urldate = {2011-10-23},
	journal = {Radiographics},
	author = {Mortelé, Koenraad J. and Rocha, Tatiana C. and Streeter, Jonathan L. and Taylor, Andrew J.},
	month = jun,
	year = {2006},
	pages = {715 --731},
	file = {Mortelé et al. - 2006 - Multimodality Imaging of Pancreatic and Biliary Co.pdf:/home/denest/zoterolib/storage/MV9AE8R2/Mortelé et al. - 2006 - Multimodality Imaging of Pancreatic and Biliary Co.pdf:application/pdf}
}

