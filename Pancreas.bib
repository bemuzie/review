}

@article{joergensen_comparison_2010,
	title = {Comparison of plasma Tu-M2-{PK} and {CA19-9} in pancreatic cancer},
	volume = {39},
	issn = {1536-4828},
	doi = {10.1097/MPA.0b013e3181bae8ab},
	abstract = {{OBJECTIVES:} The performance of the 2 tumor markers carbohydrate antigen 19-9 ({CA19-9)} and tumor M2 pyruvate kinase (Tu-M2-{PK)} separately and in combination detecting pancreatic ductal adenocarcinoma ({PDAC)} was evaluated in a prospective study.
{METHODS:} The study comprised 103 patients referred because of suspicion of pancreatic cancer. Of these, 51 patients had their conditions diagnosed as {PDAC}, whereas this diagnosis was ruled out in 52 after 12 months of follow-up. The performance of Tu-M2-{PK} was compared with that of {CA19-9} using cutoff values 15 and 37 {U/mL}, respectively.
{RESULTS:} The sensitivity of Tu-M2-{PK} and {CA19-9} in detecting {PDAC} was 55\% and 86\% at specificities of 52\% and 73\%, respectively. The area under the curve ({AUC)} of Tu-M2-{PK} was 0.55 and that of {CA19-9} was 0.84. Combining the 2 markers did not significantly improve {AUC} ({AUC} = 0.85, P = 0.72) compared with {CA19-9} when used alone. The presence of chronic pancreatitis or jaundice causes increased levels of {CA19-9} but does not influence Tu-M2-{PK.}
{CONCLUSIONS:} Tu-M2-{PK} was inferior to {CA19-9} as marker of {PDAC.} Tu-M2-{PK} may have a role in diagnosing {PDAC} because it is not affected by cholestasis or Lewis phenotype. Neither tumor marker can stand alone in the diagnosis of {PDAC.}},
	language = {eng},
	number = {2},
	journal = {Pancreas},
	author = {Joergensen, Maiken Thyregod and Heegaard, Niels H H and Schaffalitzky de Muckadell, Ove B},
	month = mar,
	year = {2010},
	note = {{PMID:} 19820423},
	keywords = {Aged, {CA-19-9} Antigen, Carcinoma, Pancreatic Ductal, Case-Control Studies, Chi-Square Distribution, Female, Humans, Logistic Models, Male, Middle Aged, Neoplasm Staging, Odds Ratio, Pancreatic Neoplasms, Predictive Value of Tests, Prospective Studies, Pyruvate Kinase, {ROC} Curve, Sensitivity and Specificity, Tumor Markers, Biological},
