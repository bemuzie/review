	keywords = {Adult, Aged, Aged, 80 and over, Angiography, Aortography, Contrast Media, Female, Humans, Image Processing, Computer-Assisted, Male, Middle Aged, Pancreas, Pancreatic Neoplasms, Tomography, X-Ray Computed},
