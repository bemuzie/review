	keywords = {Adult, Aged, Aged, 80 and over, Contrast Media, Female, Humans, Image Processing, Computer-Assisted, Male, Middle Aged, Pancreatic Neoplasms, Radiography, Abdominal, Sensitivity and Specificity, Tomography, Spiral Computed},
	pages = {149--156},
