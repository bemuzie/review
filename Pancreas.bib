@article{hayashi_rapid_2013,
	title = {Rapid on-site evaluation by endosonographer during endoscopic ultrasound-guided fine needle aspiration for pancreatic solid masses},
	volume = {28},
	issn = {1440-1746},
	doi = {10.1111/jgh.12122},
	abstract = {{BACKGROUND} {AND} {AIM:} Endoscopic ultrasound-guided fine needle aspiration ({EUS-FNA)} is an established diagnostic method for patients with suspected pancreatic ductal carcinoma. Rapid on-site evaluation ({ROSE)} has been reported to improve the accuracy. However, an on-site cytopathologist is not routinely available in many institutions. One of the solutions may be {ROSE} by endosonographer. The aim was to examine whether diagnostic accuracy increases through {ROSE} by endosonographer using our cytological criteria.
{METHODS:} Patients who underwent {EUS-FNA} of solid pancreatic masses from January 2006 to August 2009 (n = 53, period 1) and September 2009 to April 2011 (n = 85, period 2) were retrospectively identified. Before initiating {ROSE} at the start of period 2, two endosonographers underwent training for cytological interpretation, which was focused on four cytological features of pancreatic ductal carcinoma: anisonucleosis, nuclear membrane irregularity, overlapping, and enlargement. During {EUS-FNA} in period 2, endosonographers classified the Diff-Quik smears under three atypical grades and evaluated the adequacy. All diagnoses were made by one pathologist without knowledge of clinical information.
{RESULTS:} The rate of "inconclusive" diagnoses, interpreted as "suspicious," "atypical," and "inadequate for diagnosis" was reduced from 26.4\% in period 1 to 8.2\% in period 2 (P = 0.004). Moreover, diagnostic accuracy was increased from 69.2\% in period 1 to 91.8\% in period 2 (P {\textless} 0.001).
{CONCLUSIONS:} This cytological grading system used in {ROSE} by endosonographers is invaluable for the diagnosis of pancreatic solid masses.},
	language = {eng},
	number = {4},
	journal = {Journal of gastroenterology and hepatology},
	author = {Hayashi, Tsuyoshi and Ishiwatari, Hirotoshi and Yoshida, Makoto and Ono, Michihiro and Sato, Tsutomu and Miyanishi, Koji and Sato, Yasushi and Kobune, Masayoshi and Takimoto, Rishu and Mitsuhashi, Tomoko and Asanuma, Hiroko and Ogino, Jiro and Hasegawa, Tadashi and Sonoda, Tomoko and Kato, Junji},
	month = apr,
