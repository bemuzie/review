@article{__2013-6,
	title = {Роль ультразвукового исследования и малоинвазивных вмешательств под контролем сонографии в диагностике рака поджелудочной железы},
	issn = {2079-5343},
	abstract = {Рак поджелудочной железы имеет один из самых неблагоприятных прогнозов. Статистика заболеваемости свидетельствует о неуклонном росте частоты рака поджелудочной железы в развитых странах за последние десятилетия. По данным мировой статистики большинство (до 80\%) злокачественных опухолей выявляется при местнораспространенном процессе либо при наличии отдаленных метастазов. Ранняя и точная диагностика заболевания является важным фактором планирования адекватного лечения пациентов с локализованными, местнораспространенными опухолями и при наличии отдаленных метастазов. Зачастую первым диагностическим методом в такой группе пациентов служит трансабдоминальное ультразвуковое исследование (УЗИ, сонография). В статье описаны возможности современной сонографии в стадировании и морфологической верификации рака поджелудочной железы.},
	language = {русский},
	number = {3 (4)},
	journal = {ЛУЧЕВАЯ ДИАГНОСТИКА И ТЕРАПИЯ},
	author = {СТЕПАНОВ, С. О. and СИДОРОВ, Д. В. and СКРЕПЦОВА, Н. С. and ПРОЗОРОВА, ЭЛИНА В.},
	year = {2013},
	keywords = {Biopsy, pancreatic cancer, {SONOGRAPHY}, {ULTRASOUND} {DIAGNOSIS}, БИОПСИЯ, РАК ПОДЖЕЛУДОЧНОЙ ЖЕЛЕЗЫ, УЛЬТРАЗВУКОВАЯ ДИАГНОСТИКА},
