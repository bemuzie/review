	note = {{PMID:} 23890797 
{PMCID:} {PMC3752990}},
	keywords = {Aged, Body Mass Index, Confidence Intervals, Dietary Fats, Diet Surveys, Early Detection of Cancer, Follow-Up Studies, Food Habits, Humans, Incidence, Male, Middle Aged, Pancreatic Neoplasms, Proportional Hazards Models, Prospective Studies, Questionnaires, Risk Factors, Socioeconomic Factors},
