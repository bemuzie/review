	keywords = {Adult, Aged, Female, Humans, Imaging, Three-Dimensional, Male, Middle Aged, Observer Variation, Pancreatic Neoplasms, Retrospective Studies, Sensitivity and Specificity, Time Factors, Tomography, X-Ray Computed},
