	keywords = {Adenocarcinoma, Deoxyglucose, Female, Fluorodeoxyglucose F18, Humans, Male, Middle Aged, Pancreatic Neoplasms, Tomography, Emission-Computed, Tomography, X-Ray Computed},
	pages = {596--601}
