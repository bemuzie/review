	journal = {Annals of Surgery},
	author = {Trede, M and Rumstadt, B and Wendl, K and Gaa, J and Tesdal, K and Lehmann, K J and Meier-Willersen, H J and Pescatore, P and Schmoll, J},
	month = oct,
	year = {1997},
	note = {{PMID:} 9351708
{PMCID:} {PMC1191049}},
	pages = {393--407},
	file = {Trede et al. - 1997 - Ultrafast magnetic resonance imaging improves the .pdf:/home/denest/zoterolib/storage/XGMZR9UH/Trede et al. - 1997 - Ultrafast magnetic resonance imaging improves the .pdf:application/pdf}
}

