	pages = {137--144}
}

@article{krishna_small_2012,
	title = {Small and potentially resectable focal pancreatic lesions noted on {CT/MRI} scans in nonjaundiced patients: likelihood of neoplasia and utility of {EUS.}},
	volume = {16},
	doi = {10.1007/s11605-011-1812-3},
	abstract = {{BACKGROUND:} Management of small and potentially resectable "mass" lesions encountered on {CT/MRI} scans in patients without obstructive jaundice ({ObJ)} is rather empirical since there is scant data on likelihood of neoplasm to formulate treatment strategies. We evaluated (1) the prevalence of neoplasm and (2) performance characteristics of {EUS-FNA} for diagnosing neoplasm in above-mentioned subset of patients. {PATIENTS:} This is a retrospective analysis of 232 patients (without {ObJ)} with a focal pancreatic lesion, {\textless}/= 25 mm and potentially resectable on {CT/MRI} who underwent {EUS-FNA} from 2002 to 2009. {RESULTS:} Seventy-five patients (32.3\%, 95\% {CI} 26.6, 38.6) were finally diagnosed to have a neoplasm. Four of 92 (4.3\%) lesions {\textless}/= 15 mm, 13 of 57(22.8\%) lesions 16-20 mm,  and 35 of 83 (42.1\%) lesions 21-25 mm had an adenocarcinoma. Larger lesion size,  older patient age, and h/o recent weight loss significantly increased the likelihood of adenocarcinoma. {EUS-FNA} had 98.2\% overall accuracy and 98.1\% {NPV} with no significant differences based on lesion size. {CONCLUSIONS:} In nonjaundiced patient with a potentially resectable pancreatic lesion {\textless}/=25 mm in  size noted on {CT/MRI} scanning, {EUS-FNA} can provide useful adjunctive information  to optimize the use of surgery and can potentially obviate the need for "wait and watch approach" with repeat imaging in their clinical management.},
	language = {eng},
	number = {4},
	journal = {Journal of gastrointestinal surgery : official journal of the Society for Surgery of the Alimentary Tract},
	author = {Krishna, Naveen B. and Tummala, Pavan and Mehan, Christopher D. and Reddy, Amith V. and Hartman, Jeremy A. and Agarwal, Banke},
	month = apr,
	year = {2012},
	note = {{PMID:} 22258870},
	keywords = {Adenocarcinoma, Adenocarcinoma/diagnosis/*pathology/surgery, Adult, Aged, Aged, 80 and over, Age Factors, Analysis of Variance, Area Under Curve, Biopsy, Fine-Needle, Chi-Square Distribution, Confidence Intervals, {CT} scan, Endoscopic ultrasound, Endosonography, {*Endosonography}, Female, Gastroenterology, Humans, Incidental Findings, Likelihood of neoplasm, Magnetic Resonance Imaging, Male, Middle Aged, {MRI} scan, Neuroendocrine Tumors, Neuroendocrine Tumors/diagnosis/*pathology/surgery, Odds Ratio, pancreatic cancer, Pancreatic Neoplasms, Pancreatic Neoplasms/diagnosis/*pathology/surgery, Retrospective Studies, Surgery, Tomography, X-Ray Computed, Tumor Burden, tumor size, {*Ultrasonography}, Interventional, Ultrasonography, Interventional, Weight Loss, Young Adult},
