	note = {{PMID:} 20657218},
	keywords = {Adolescent, Adult, Aged, Contrast Media, Contrast Media/diagnostic use, Diagnosis, Differential, Female, Gadolinium {DTPA}, Gadolinium {DTPA/diagnostic} use, Humans, Image Enhancement, Image Enhancement/methods, Iohexol, Iohexol/analogs \& derivatives/diagnostic use, Magnetic Resonance Imaging, Magnetic Resonance Imaging/methods, Male, Middle Aged, Pancreas, Pancreas/pathology/radiography, Pancreatic Diseases, Pancreatic Diseases/pathology/radiography, Pancreatic Neoplasms, Pancreatic Neoplasms/*pathology/*radiography, Tomography, X-Ray Computed, Tomography, X-Ray Computed/methods, von Hippel-Lindau Disease, von Hippel-Lindau Disease/*pathology/*radiography, Young Adult},
