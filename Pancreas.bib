	number = {2},
	journal = {Annals of surgery},
	author = {Heinrich, Stefan and Goerres, Gerhard W and Schäfer, Markus and Sagmeister, Markus and Bauerfeind, Peter and Pestalozzi, Bernhard C and Hany, Thomas F and von Schulthess, Gustav K and Clavien, Pierre-Alain},
