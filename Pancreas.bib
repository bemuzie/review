	number = {2},
	journal = {Journal of magnetic resonance imaging: {JMRI}},
	author = {Fattahi, Rana and Balci, N Cem and Perman, William H and Hsueh, Eddy C and Alkaade, Samer and Havlioglu, Necat and Burton, Frank R},
