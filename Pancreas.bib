	author = {Li, S and Peck-Radosavljevic, M and Kienast, O and Preitfellner, J and Havlik, E and Schima, W and Traub-Weidinger, T and Graf, S and Beheshti, M and Schmid, M and Angelberger, P and Dudczak, R},
	month = sep,
	year = {2004},
