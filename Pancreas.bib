@article{connor_serum_2005,
	title = {Serum {CA19-9} measurement increases the effectiveness of staging laparoscopy in patients with suspected pancreatic malignancy},
	volume = {22},
	issn = {0253-4886},
	doi = {10.1159/000085297},
	abstract = {{BACKGROUND/AIMS:} Staging laparoscopy for suspected pancreatic neoplasia is not widely accepted due to its low yield. The aim of this study was to determine if serum carbohydrate antigen ({CA19-9)} levels could be used to improve the selection of patients for staging laparoscopy.
{METHODS:} The data from a prospectively collected database (1997-2004) with 159 patients who had computed tomography-predicted resectable disease and who had undergone laparoscopic staging were analysed to determine if a low preoperative {CA19-9} level ({\textless} or =150 {kU/l}, or {\textless} or =300 {kU/l} with a bilirubin {\textgreater}35 micromol/l) identified patients in whom laparoscopy was not useful.
{RESULTS:} The {CA19-9} level was {\textgreater}150 {kU/l} in 96 patients of whom 75 (78\%) were considered resectable following laparoscopic assessment. There were 63 patients with a {CA19-9} {\textless} or =150 {kU/l} of whom 60 (95\%) were considered resectable following laparoscopic assessment. The sensitivity, specificity, positive predictive value and negative predictive value for {CA19-9} {\textless} or =150 {kU/l} in predicting that laparoscopic assessment would judge patients as resectable were 44, 88, 95 and 22\%, respectively. A cut-off level of {\textless} or =300 {kU/l} in patients with a bilirubin {\textgreater}35 micromol/l produced values of 30, 94, 94 and 28\%, respectively. By using {CA19-9} {\textless} or =150 {kU/l}, laparoscopy could have been avoided in 40\% of patients, increased to 55\% of patients with adjustment for the presence of jaundice; concomitantly, the yield from laparoscopy would have been increased from 15 to 22 and 25\%, respectively.
{CONCLUSION:} Use of serum {CA19-9} levels would increase the efficiency of laparoscopic staging in patients with suspected pancreatic malignancy.},
	language = {eng},
	number = {1-2},
	journal = {Digestive surgery},
	author = {Connor, S and Bosonnet, L and Alexakis, N and Raraty, M and Ghaneh, P and Sutton, R and Neoptolemos, J P},
	year = {2005},
	note = {{PMID:} 15849467},
	keywords = {Bile Duct Neoplasms, Bile Ducts, Intrahepatic, Bilirubin, {CA-19-9} Antigen, Carcinoma, Pancreatic Ductal, Cholangiocarcinoma, Humans, Laparoscopy, Neoplasm Staging, Pancreatic Neoplasms, Patient Selection, Sensitivity and Specificity},
	pages = {80--85}
}

@article{ni_clinical_2005,
	title = {The clinical value of serum {CEA}, {CA19-9}, and {CA242} in the diagnosis and prognosis of pancreatic cancer},
	volume = {31},
	issn = {0748-7983},
	doi = {10.1016/j.ejso.2004.09.007},
	abstract = {{AIM:} Serum tumour markers carcinoembryonic antigen ({CEA)}, carbohydrate antigen 19-9 ({CA19-9)} and {CA242} were investigated to evaluate the values of single and combined test in the diagnosis and prognosis of pancreatic cancer.
{METHODS:} Pre-operative serum {CEA}, {CA19-9} and {CA242} were measured in 105 pancreatic cancers, 70 non-pancreatic malignancies and 30 benign pancreatic diseases.
{RESULTS:} The sensitivity of {CA19-9} alone was the highest in pancreatic cancer patients (80\%), but the specificity was significantly lower than that of {CEA} and {CA242} (P{\textless}0.01). The combination of {CEA} and {CA242} could increase the specificity to 92\%. In serum {CA242} positive patients, the survival time was remarkably shorter than that of patients with negative result (P{\textless}0.01). The survival time in patients with more than two markers positive expression of {CEA}, {CA19-9} and {CA242} was obviously shorter than that of only one or no marker positive expression (P{\textless}0.05).
{CONCLUSION:} The diagnostic rate of {CA19-9} in pancreatic cancer is better than that of {CEA} and {CA242.} Combined detection of {CEA} and {CA242} can improve the diagnostic specificity obviously. High levels of serum markers are associated with advanced stage of the disease. Patients with two or three markers positive expression of {CEA}, {CA19-9}, and {CA242} simultaneously had a shorter survival time.},
	language = {eng},
	number = {2},
	journal = {European journal of surgical oncology: the journal of the European Society of Surgical Oncology and the British Association of Surgical Oncology},
	author = {Ni, X G and Bai, X F and Mao, Y L and Shao, Y F and Wu, J X and Shan, Y and Wang, C F and Wang, J and Tian, Y T and Liu, Q and Xu, D K and Zhao, P},
	month = mar,
