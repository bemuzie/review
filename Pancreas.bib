	pages = {1981--1990},
	file = {Kartalis et al. - 2009 - Diffusion-weighted magnetic resonance imaging of p.pdf:/home/denest/zoterolib/storage/SDSUS3ZI/Kartalis et al. - 2009 - Diffusion-weighted magnetic resonance imaging of p.pdf:application/pdf}
