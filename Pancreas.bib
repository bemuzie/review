	note = {{PMID:} 15929774},
	keywords = {Adenocarcinoma, Adult, Aged, Aged, 80 and over, Celiac Artery, Diagnosis, Differential, Endosonography, Female, Follow-Up Studies, Humans, Male, Mesenteric Veins, Middle Aged, Monitoring, Intraoperative, Neoplasm Invasiveness, Neoplasm Staging, Palpation, Pancreatectomy, Pancreatic Neoplasms, Portal Vein, Predictive Value of Tests, Retrospective Studies, Sensitivity and Specificity, Vena Cava, Inferior, Videotape Recording},
