	keywords = {Computed tomography, pancreatic cancer, {THE} {DIAGNOSIS} {OF} {PANCREATIC} {CANCER}, ДИАГНОСТИКА РАКА ПОДЖЕЛУ ДОЧНОЙ ЖЕЛЕЗЫ, КОМПЬЮТЕРНАЯ ТОМОГРАФИЯ, ОПУХОЛЬ ПОДЖЕЛУДОЧНОЙ ЖЕЛЕЗЫ},
	pages = {51--56}
}

