	keywords = {Acute Disease, Biopsy, Fine-Needle, Chronic Disease, Humans, Pancreatic Diseases, Pancreatic Neoplasms, Pancreatitis, Positron-Emission Tomography, Tomography, X-Ray Computed},
	pages = {211--220}
}

