@article{stotz_clinico-pathological_2013,
	title = {Clinico-pathological characteristics and clinical outcome of different histological types of pancreatic cancer in a large Middle European series},
	volume = {66},
	issn = {1472-4146},
	doi = {10.1136/jclinpath-2012-201394},
	abstract = {{AIMS:} Pancreatic cancer ({PC)} is a heterogeneous disease in terms of histological and molecular subtypes. The aim of this study was to evaluate the prognostic impact of different histological subtypes on cancer-specific survival ({CSS)} in a large single-centre Middle European cohort.
{METHODS:} We retrospectively studied the records of 400 consecutive {PC} patients who were treated from 2004 to 2010 at a single tertiary academic centre. The association of histological subtypes and parameters such as tumour stage, tumour grade, levels of tumour markers carcinoembryonic antigen and {CA19-9} at diagnosis, was studied. {CSS} was calculated using the Kaplan-Meier method, and the influence of each parameter on {CSS} was assessed with univariate and multivariable Cox proportional models.
{RESULTS:} The survival time was significantly shorter in the ductal adenocarcinoma and acinar histological subtypes compared to neuroendocrine differentiation (p{\textless}0.001). No survival difference was observed between ductal adenocarcinomas and patients with a histological variant of ductal adenocarcinoma, namely, mucinous non-cystic adenocarcinoma (p=0.7). In multivariable analysis, ductal adenocarcinoma ({HR=3.1}, {CI} 1.6 to 6.1, p=0.001) and acinar carcinoma ({HR=3.2}, {CI} 1.3 to 8.5, p=0.016) were identified as independent predictors for {CSS.}
{CONCLUSIONS:} Our findings suggest that the main histological subtype is an independent predictor of {CSS} in patients with {PC.} Thus, our data underline the importance of routine assessment of histological type in {PC} for individual risk assessment. However, no clinical rationale for the subdivision of ductal adenocarcinoma and mucinous non-cystic adenocarcinoma can be supported by our study.},
	language = {eng},
	number = {9},
	journal = {Journal of clinical pathology},
	author = {Stotz, Michael and Eisner, Florian and Szkandera, Joanna and Absenger, Gudrun and Kornprat, Peter and Lackner, Carolin and Samonigg, Hellmut and Gerger, Armin and Pichler, Martin},
	month = sep,
	year = {2013},
	note = {{PMID:} 23750038},
	keywords = {Adenocarcinoma, Mucinous, Aged, Austria, {CA-19-9} Antigen, Carcinoma, Acinar Cell, Carcinoma, Pancreatic Ductal, Female, Humans, Male, Middle Aged, Neoplasm Grading, Neoplasm Staging, Pancreatic Neoplasms, Prognosis, Retrospective Studies, Survival Rate, Treatment Outcome, Tumor Markers, Biological},
	pages = {753--757}
}

