	keywords = {Adolescent, Adult, Aged, Aged, 80 and over, Female, Humans, Male, Middle Aged, Neuroendocrine Tumors, Organometallic Compounds, Pancreatic Neoplasms, Radiopharmaceuticals, Reproducibility of Results, Sensitivity and Specificity, Tissue Distribution, Young Adult},
