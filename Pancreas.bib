	number = {4},
	journal = {Pancreas},
	author = {Tellez-Avila, Felix I and Chavez-Tapia, Norberto C and López-Arce, Gustavo and Franco-Guzmán, Ada M and Sosa-Lozano, Luis Antonio and Alfaro-Lara, Roberto and Chan-Nuñez, Carlos and Giovannini, Marc and Elizondo-Rivera, Javier and Ramírez-Luna, Miguel Angel},
	month = may,
