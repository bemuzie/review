	note = {{PMID:} 16822803},
	keywords = {Adenocarcinoma, Adult, Aged, Constriction, Pathologic, Female, Humans, Male, Middle Aged, Neoplasm Invasiveness, Pancreatic Neoplasms, Tomography, X-Ray Computed},
	pages = {880--887}
}

@article{thomson_refining_2006,
