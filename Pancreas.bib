	keywords = {Adenocarcinoma, Adult, Aged, Aged, 80 and over, Canada, Early Detection of Cancer, Female, Follow-Up Studies, Humans, Magnetic Resonance Imaging, Male, Middle Aged, Neuroendocrine Tumors, Pancreatic Cyst, Pancreatic Neoplasms, Prospective Studies, Young Adult},
