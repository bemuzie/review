	author = {ХРИСТИЧ, Т. Н.},
	year = {2013},
	keywords = {{CHRONIC} {PANCREATITIS}, {NONALCOHOLIC} {FATTY} {PANCREAS} {DISEASE}, {PANCREAS} {CANCER}, НЕАЛКОГОЛЬНА ЖИРОВА ХВОРОБА ПіДШЛУНКОВОї ЗАЛОЗИ, НЕАЛКОГОЛЬНАЯ ЖИРОВАЯ БОЛЕЗНЬ ПОДЖЕЛУДОЧНОЙ ЖЕЛЕЗЫ, РАК ПіДШЛУНКОВОї ЗАЛОЗИ, РАК ПОДЖЕЛУДОЧНОЙ ЖЕЛЕЗЫ, ХРОНИЧЕСКИЙ ПАНКРЕАТИТ, ХРОНіЧНИИЙ ПАНКРЕАТИТ, ЦИТОКИНЫ, ЦИТОКіНИ},
	pages = {15--20},
