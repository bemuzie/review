	pages = {445--453},
	file = {Bartsch et al. - 2012 - Familial pancreatic cancer--current knowledge..pdf:/home/denest/zoterolib/storage/TEGPCXT5/Bartsch et al. - 2012 - Familial pancreatic cancer--current knowledge..pdf:application/pdf}
