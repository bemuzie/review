	keywords = {Adult, Aged, Aged, 80 and over, Cholangiopancreatography, Magnetic Resonance, Cholangiopancreatography, Magnetic Resonance/methods, Contrast Media, Contrast Media/*diagnostic use, Female, Humans, Image Enhancement, Image Enhancement/methods, Imaging, Three-Dimensional, Imaging, Three-Dimensional/methods, Iohexol, Iohexol/analogs \& derivatives/diagnostic use, Magnetic Resonance Imaging, Magnetic Resonance Imaging/*methods, Male, Meglumine, Meglumine/*analogs \& derivatives/diagnostic use, Middle Aged, Neoplasm Staging, Observer Variation, Organometallic Compounds, Organometallic Compounds/*diagnostic use, Pancreas, Pancreas/pathology/radiography/surgery, Pancreatic Neoplasms, Pancreatic Neoplasms/*pathology/*radiography/surgery, Preoperative Care, Preoperative Care/*methods, Reproducibility of Results, Retrospective Studies, {ROC} Curve, Sensitivity and Specificity, Tomography, X-Ray Computed, Tomography, X-Ray Computed/*methods},
	pages = {586--595},
