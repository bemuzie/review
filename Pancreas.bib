	urldate = {2012-02-27},
	journal = {The New England Journal of Medicine},
	author = {Lowenfels, A B and Maisonneuve, P and Cavallini, G and Ammann, R W and Lankisch, P G and Andersen, J R and Dimagno, E P and Andrén-Sandberg, A and Domellöf, L},
	month = may,
