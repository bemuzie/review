	author = {Park, Hyung-Doo and Kang, Eun-Suk and Kim, Jong-Won and Lee, Kyu-Taek and Lee, Kwang Hyuck and Park, Young Suk and Park, Joon-Oh and Lee, Jeeyun and Heo, Jin Seok and Choi, Seong Ho and Choi, Dong Wook and Kim, Seonwoo and Lee, Jong Kyun and Lee, Soo-Youn},
