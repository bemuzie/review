	note = {{PMID:} 17004868},
	keywords = {Adenocarcinoma, Adult, Aged, Aged, 80 and over, Female, Humans, Laparoscopy, Male, Middle Aged, Neoplasm Staging, Pancreatic Neoplasms, Tomography, X-Ray Computed},
	pages = {458--463}
