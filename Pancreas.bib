	keywords = {Adult, Case-Control Studies, Chronic Disease, Cohort Studies, Confidence Intervals, Europe, Female, Humans, Incidence, Male, Middle Aged, Pancreatic Neoplasms, Pancreatitis, Proportional Hazards Models, Risk Factors, United States},
	pages = {1433--1437},
	file = {Lowenfels et al_1993_Pancreatitis and the risk of pancreatic cancer.pdf:/home/denest/zoterolib/storage/8FTN7URH/Lowenfels et al_1993_Pancreatitis and the risk of pancreatic cancer.pdf:application/pdf}
}

