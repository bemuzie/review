	journal = {Abdominal imaging},
	author = {Holzapfel, Konstantin and Reiser-Erkan, Carolin and Fingerle, Alexander A and Erkan, Merd and Eiber, Matthias J and Rummeny, Ernst J and Friess, Helmut and Kleeff, Jörg and Gaa, Jochen},
