	note = {{PMID:} 18329114},
	keywords = {Adenocarcinoma, Aged, Aged, 80 and over, {CA-19-9} Antigen, Case-Control Studies, Chromogranin A, Female, Humans, Male, Neoplasm Staging, Pancreatic Neoplasms, Pancreatitis, Chronic, Prognosis, Tumor Markers, Biological},
