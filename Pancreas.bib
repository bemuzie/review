	keywords = {Aged, Aged, Carcinoma, Acinar Cell, Carcinoma, Acinar Cell, Female, Female, Humans, Humans, Male, Male, Middle Aged, Middle Aged, Pancreatic Neoplasms, Pancreatic Neoplasms, Retrospective Studies, Retrospective Studies, Tomography, X-Ray Computed, Tomography, X-Ray Computed},
	pages = {137--143},
	file = {s00261-012-9868-4.pdf:/home/denest/zoterolib/storage/NZ9EA9PJ/s00261-012-9868-4.pdf:application/pdf;s00261-012-9868-4.pdf:/home/denest/zoterolib/storage/ZWZ6TUBD/s00261-012-9868-4 (1).pdf:application/pdf}
}

