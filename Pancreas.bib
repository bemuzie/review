	keywords = {Adult, Aged, Aged, 80 and over, Analysis of Variance, Autoimmune Diseases, Carcinoma, Pancreatic Ductal, Contrast Media, Diagnosis, Differential, Endosonography, Female, Ferric Compounds, Humans, Iron, Japan, Male, Middle Aged, Oxides, Pancreatic Diseases, Pancreatic Neoplasms, Pancreatitis, Predictive Value of Tests, Retrospective Studies, {ROC} Curve, Time Factors},
	pages = {1073--1079}
