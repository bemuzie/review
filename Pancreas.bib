	note = {{PMID:} 19239583},
	keywords = {Adult, Aged, Bayes Theorem, Female, Humans, Likelihood Functions, Male, Mexico, Middle Aged, Pancreatic Neoplasms, {ROC} Curve, Technology Assessment, Biomedical},
	pages = {62--68}
}

