	note = {{PMID:} 21155879},
	keywords = {Adult, Aged, Aged, 80 and over, Biopsy, Chi-Square Distribution, Contrast Media, False Negative Reactions, False Positive Reactions, Female, Fluorodeoxyglucose F18, Humans, Image Interpretation, Computer-Assisted, Imaging, Three-Dimensional, Male, Middle Aged, Neoplasm Staging, Pancreatic Neoplasms, Pilot Projects, Positron-Emission Tomography, Predictive Value of Tests, Preoperative Care, Prospective Studies, Radiopharmaceuticals, Sensitivity and Specificity, Switzerland, Tomography, X-Ray Computed, Young Adult},
