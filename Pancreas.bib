	journal = {Journal of vascular and interventional radiology: {JVIR}},
	author = {Li, Li and Liu, Li-Zhi and Wu, Qiu-Liang and Mo, Yun-Xian and Liu, Xue-Wen and Cui, Chun-Yan and Wan, De-Sen},
	month = jan,
