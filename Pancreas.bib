	keywords = {Aged, Aged, 80 and over, Computed tomography, Diagnosis, Diagnosis, Differential, Diagnostic Imaging, Endoscopic ultrasonography, {EUS}, Female, Follow-Up Studies, Gastroenterology, Hepatology, Humans, Image Processing, Computer-Assisted, Imaging / Radiology, Male, {MDCT}, Middle Aged, pancreatic cancer, Pancreatic Neoplasms, Radiology, Retrospective Studies, {ROC} Curve, Tomography, X-Ray Computed},
	pages = {660--667},
