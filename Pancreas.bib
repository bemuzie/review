}

@article{takagi_analysis_2008,
	title = {Analysis of microvessels in pancreatic cancer: by light microscopy, confocal laser scan microscopy, and electron microscopy},
	volume = {15},
	issn = {0944-1166, 1436-0691},
	shorttitle = {Analysis of microvessels in pancreatic cancer},
	url = {http://link.springer.com/article/10.1007/s00534-007-1241-6},
	doi = {10.1007/s00534-007-1241-6},
	abstract = {{Background/Purpose} The microvessel density ({MVD)} of most malignant tumors is considered to be strongly related to metastasis and prognosis. Weidner’s “hot spot method” for determining {MVD} is in general use, but it is possible that cells other than endothelial cells will also be stained. In our previous study, no correlations were observed between {MVD} determined by the “hot spot method” and prognosis/metastasis. But, using the “lumen method,” we found a correlation with the number of vessel structures only. In the present study, we analyzed the staining of microvessels in pancreatic cancer, using light microscopy, confocal laser scan microscopy ({CLSM)}, and transmission electron microscopy ({TEM).} Methods Microvessel staining of pancreatic cancer with {CD34}, factor {VIII}, and {CD45} antibodies was examined in consecutive slices by light microscopy. For {CLSM}, freshly resected specimens were immunostained with factor {VIII} and fluorescein isothiocynate. For {TEM}, specimens were fixed with 2.5\% glutaraldehyde, treated with 1\% osmium tetroxide, and embedded in epoxy resin. Results Staining of vessels with {CD34} and factor {VIII} antibodies appeared similar under light microscopy. However, {CD34-stained} consecutive slices were judged not to reveal vessel structures, and some cells stained with {CD45} antibody were similar in appearance to {CD34-stained} cells. Under {CLSM}, irregular arrangements of neovascularization, consisting of many branches, were observed, but many positively stained cells not identified as vessels were also seen. Microvessels were distinctly identified under {TEM}, but the types of individual cells could not be determined. Conclusions An integrated, reproducible method for the measurement of {MVD} is vital. For pancreatic cancer, the “lumen method” is recommended.},
	language = {en},
	number = {4},
	urldate = {2013-12-30},
	journal = {Journal of Hepato-Biliary-Pancreatic Surgery},
	author = {Takagi, Kenji and Takada, Tadahiro and Amano, Hodaka and Yoshida, Masahiro and Miura, Humihiko and Toyota, Naoyuki and Wada, Keita and Takahashi, Ichirou},
	month = jul,
	year = {2008},
	keywords = {Abdominal Surgery, Angiogenesis, Confocal laser scan microscopy, Diagnostic Radiology, Electron microscopy, Gastroenterology, Microvessel density, pancreatic cancer, Pathology, Surgical Oncology},
	pages = {384--390},
	file = {Snapshot:/home/denest/zoterolib/storage/VKMDH3R3/10.html:text/html}
}

@article{nanashima_relationship_2012,
	title = {Relationship between microvessel count and clinicopathological characteristics and postoperative survival in patients with pancreatic carcinoma},
	volume = {59},
	issn = {0172-6390},
	doi = {10.5754/hge10618},
	abstract = {{BACKGROUND/AIMS:} The present study aimed to elucidate the relationship between microvessel count ({MVC)} according to {CD34} expression and clinicopathological characteristics or prognosis in pancreatic carcinoma ({PC)} patients who underwent hepatectomy.
{METHODOLOGY:} {CD34} expression was analyzed using immunohistochemical methods. Mean {MVC} in 5 areas per specimen and clinicopathological factors were consecutively examined in 42 {PC} patients.
{RESULTS:} Median {MVC} for {PC} patients was 123/mm2, which was applied as a cut-off value. Higher {MVC} was significantly associated with the advanced Japanese tumor-node-metastasis stage {IVa} and {IVb} (p=0.034). Univariate survival analysis identified higher carcinoembryonic antigen ({CEA)} and {CA19-9} level, infiltrative type on macroscopic examination, invasive ductal carcinoma, node metastasis and higher tumor-node-metastasis classification were significantly associated with poor survival. The 5-year overall survival rate in the higher {MVC} group tended to be lower than that in the higher {MVC} group (37 vs. 55\%), but not statistically significant (p=0.15).
{CONCLUSIONS:} Tumor {MVC} might be a candidate prognostic marker of {PC} patient survival after pancreatectomy and further investigation in a larger series is warranted to clarify the significance of this marker.},
