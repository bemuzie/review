	note = {{PMID:} 16989906},
	keywords = {Adjustment Disorders, Aged, Blood Glucose, Brain, Energy Metabolism, Female, Fluorodeoxyglucose F18, Gyrus Cinguli, Humans, Limbic System, Male, Middle Aged, Pancreatic Neoplasms, Positron-Emission Tomography, Prefrontal Cortex, Prognosis},
