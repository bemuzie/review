	author = {Bhatia, Prateek and Srinivasan, Radhika and Rajwanshi, Arvind and Nijhawan, Raje and Khandelwal, Niranjan and Wig, Jaidev and Vasishtha, Rakesh K},
	month = oct,
	year = {2008},
	note = {{PMID:} 18833812},
	keywords = {Adolescent, Adult, Aged, Biopsy, Fine-Needle, Female, Humans, Male, Middle Aged, Pancreatic Neoplasms},
