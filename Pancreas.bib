	author = {Kobayashi, Akitoshi and Yamaguchi, Taketo and Ishihara, Takeshi and Ohshima, Tadashi and Ohno, Izumi and Seza, Katsushi and Shirai, Yoshihiko and Sudo, Kentaro and Nakagawa, Akihiko and Tadenuma, Hiroshi and Nakamura, Kazuyoshi and Saisho, Hiromitsu},
	month = mar,
