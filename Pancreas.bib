	note = {{PMID:} 19854016},
	keywords = {Endosonography, Fluorodeoxyglucose F18, Humans, Pancreatic Neoplasms, Positron-Emission Tomography, Predictive Value of Tests, Radiopharmaceuticals, Sensitivity and Specificity, Tomography, X-Ray Computed},
	pages = {142--150},
	file = {Tang et al. - 2011 - Usefulness of 18F-FDG PET, combined FDG-PETCT and.pdf:/home/denest/zoterolib/storage/MRGZGN6S/Tang et al. - 2011 - Usefulness of 18F-FDG PET, combined FDG-PETCT and.pdf:application/pdf}
