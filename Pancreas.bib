	keywords = {Adult, Aged, Aged, 80 and over, Cholangiopancreatography, Magnetic Resonance, Cholangiopancreatography, Magnetic Resonance/*methods, Diagnosis, Differential, Female, Fluorodeoxyglucose F18, Fluorodeoxyglucose F18/*diagnostic use, Follow-Up Studies, Humans, Male, Middle Aged, Neoplasm Staging, Neoplasm Staging/*methods, Pancreatic Neoplasms, Pancreatic Neoplasms/*diagnosis, Positron-Emission Tomography, Positron-Emission Tomography/*methods, Prospective Studies, Radiopharmaceuticals, Radiopharmaceuticals/*diagnostic use, Reproducibility of Results, Tomography, X-Ray Computed, Tomography, X-Ray Computed/*methods},
	pages = {957--963}
