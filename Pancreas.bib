	urldate = {2011-10-22},
	journal = {Academic Radiology},
	author = {Lu, Na and Feng, Xiao-Yuan and Hao, Si-Jie and Liang, Zong-Hui and Jin, Chen and Qiang, Jin-Wei and Guo, Qi-Yong},
	month = jan,
	year = {2011},
	note = {{PMID:} 20951612},
	keywords = {Adenocarcinoma, Adenocarcinoma, Adult, Adult, Aged, Aged, Contrast Media, Contrast Media, Diagnosis, Differential, Diagnosis, Differential, Female, Female, Humans, Humans, Image Processing, Computer-Assisted, Image Processing, Computer-Assisted, Iohexol, Iohexol, Male, Male, Middle Aged, Middle Aged, Pancreas, Pancreas, Pancreatic Neoplasms, Pancreatic Neoplasms, Pancreatitis, Chronic, Pancreatitis, Chronic, Radiographic Image Enhancement, Radiographic Image Enhancement, Tomography, Spiral Computed, Tomography, Spiral Computed},
	pages = {81--88},
