	number = {4},
	journal = {Pancreas},
	author = {Okamura, Yukiyasu and Fujii, Tsutomu and Kanzaki, Akiyuki and Yamada, Suguru and Sugimoto, Hiroyuki and Nomoto, Shuji and Takeda, Shin and Nakao, Akimasa},
