@article{schueller_multidetector_2006,
	title = {Multidetector {CT} of pancreas: effects of contrast material flow rate and individualized scan delay on enhancement of pancreas and tumor contrast},
	volume = {241},
	issn = {0033-8419},
	doi = {10.1148/radiol.2412051107},
	abstract = {{PURPOSE:} To prospectively assess whether high contrast material flow rate (8 {mL/sec)} and individualized scan delay improve enhancement of normal pancreas with multidetector computed tomography ({CT)} and, as a result, tumor-to-pancreas contrast of pancreatic adenocarcinoma.
{MATERIALS} {AND} {METHODS:} Informed consent was obtained in 40 patients (21 women, 19 men; mean age, 67.1 years); the institutional review board approved this protocol. Patients were referred for multidetector {CT} because they were suspected of having a pancreatic tumor and were randomized to receive 150 {mL} of nonionic contrast material (300 mg of iodine per milliliter) at a flow rate of 4 {mL/sec} (n = 21) or 8 {mL/sec} (n = 19). Patients underwent dynamic scanning at one level every 2 seconds for 66 seconds after intravenous administration of contrast material. Contrast enhancement of pancreas and tumors was measured with circular regions of interest (analysis of variance and Bonferroni-Holm corrected post hoc t tests).
{RESULTS:} Peak contrast enhancement in pancreas was observed significantly earlier (mean +/- standard deviation, 28.7 seconds +/- 3.5 vs 48.2 seconds +/- 5.3; P {\textless} .05) and was significantly higher (129.0 {HU} +/- 25.7 vs 106.2 {HU} +/- 35.4, P {\textless} .05) with a flow rate of 8 {mL/sec} than with a flow rate of 4 {mL/sec.} Tumor-to-pancreas contrast greater than 40 {HU} lasted significantly longer with a flow rate of 8 {mL/sec} than with a flow rate of 4 {mL/sec} (26.4 seconds +/- 11.9 vs 8.6 seconds +/- 8.3, P {\textless} .05). With a flow rate of 8 {mL/sec}, an individualized scan delay of 19 seconds after aortic transit time revealed higher tumor-to-pancreas contrast than did a fixed scan delay, and tumor conspicuity was better.
{CONCLUSION:} With 16-section {CT}, increased contrast material flow rate of 8 {mL/sec} and individualized scan delay were associated with improved pancreatic enhancement and tumor-to-pancreas contrast compared with flow rate of 4 {mL/sec} and fixed scan delay.},
	language = {eng},
	number = {2},
	journal = {Radiology},
	author = {Schueller, Gerd and Schima, Wolfgang and Schueller-Weidekamm, Claudia and Weber, Michael and Stift, Anton and Gnant, Michael and Prokesch, Rupert},
	month = nov,
	year = {2006},
	note = {{PMID:} 16982815},
	keywords = {Adenocarcinoma, Adult, Aged, Analysis of Variance, Contrast Media, Female, Humans, Iohexol, Male, Middle Aged, Pancreatic Neoplasms, Prospective Studies, Radiographic Image Interpretation, Computer-Assisted, Time Factors, Tomography, Spiral Computed},
	pages = {441--448}
}

@article{brune_multifocal_2006,
	title = {Multifocal neoplastic precursor lesions associated with lobular atrophy of the pancreas in patients having a strong family history of pancreatic cancer},
	volume = {30},
	issn = {0147-5185},
	doi = {pas.0000213265.84725.0b},
	abstract = {We screened 116 patients with a strong family history of pancreatic cancer using a combination of endoscopic ultrasound and computed tomography. Ten of these patients underwent surgical resection at our institution, providing an opportunity to define the morphology of pancreatic precursor lesions in patients with a strong family history of pancreatic cancer. Eight of the 10 pancreata were available and these were entirely submitted for histologic examination. The number of pancreatic intraepithelial neoplasia ({PanIN)} lesions and intraductal papillary mucinous neoplasms ({IPMNs)} were compared with age-matched controls. Parenchymal changes were defined. Selected precursor neoplasms from 6 pancreata were microdissected and analyzed for {KRAS} gene mutations. {PanINs} were significantly more common in the 8 cases (mean of 10.7\% of the duct profiles, range 1.0\% to 27.3\%) than in the controls (mean 1.9\%, range 0\% to 9.2\%, P{\textless}0.01). Different {KRAS} gene mutations were identified in separately microdissected precursor lesions in 2 of 6 cases. {IPMNs} were identified in 4 of the 8 cases, including 2 pancreata each having 2 distinct {IPMNs.} Both the {IPMNs} and the {PanINs}, even the low-grade {PanIN-1} lesions, were associated with lobular parenchymal atrophy. Some individuals with a strong family history of pancreatic cancer develop multifocal, noninvasive epithelial precursor lesions of the pancreas. {PanINs} and {IPMNs} produce obstructive lobular atrophy, and this atrophy is likely the source of the chronic pancreatitis-like changes seen in these patients. The multifocal nature of familial pancreatic neoplasia suggests that surveillance of these patients is warranted after partial pancreatectomy.},
	language = {eng},
	number = {9},
	journal = {The American journal of surgical pathology},
	author = {Brune, Kieran and Abe, Tadayoshi and Canto, Marcia and {O'Malley}, Lauren and Klein, Alison P and Maitra, Anirban and Volkan Adsay, N and Fishman, Elliot K and Cameron, John L and Yeo, Charles J and Kern, Scott E and Goggins, Michael and Hruban, Ralph H},
	month = sep,
	year = {2006},
	note = {{PMID:} 16931950 
{PMCID:} {PMC2746409}},
	keywords = {Adenocarcinoma, Mucinous, Adult, Aged, Atrophy, Carcinoma in Situ, Carcinoma, Papillary, Endosonography, Female, Humans, Male, Middle Aged, Mutation, Pancreas, Pancreatic Neoplasms, Precancerous Conditions, Proto-Oncogene Proteins, ras Proteins, Tomography, X-Ray Computed},
