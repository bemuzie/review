	month = apr,
	year = {2005},
	note = {{PMID:} 15549297},
	keywords = {Carcinoma, Chronic Disease, Cohort Studies, Female, Fluorodeoxyglucose F18, Humans, Male, Middle Aged, Pancreatic Neoplasms, Pancreatitis, Prognosis, Radiopharmaceuticals, Reproducibility of Results, Sensitivity and Specificity},
	pages = {399--404},
	file = {van Kouwen et al. - 2005 - FDG-PET is able to detect pancreatic carcinoma in .pdf:/home/denest/zoterolib/storage/ZSTEGPBF/van Kouwen et al. - 2005 - FDG-PET is able to detect pancreatic carcinoma in .pdf:application/pdf}
}

@article{ho_fdg-pet_1996,
	title = {{FDG-PET} evaluation of indeterminate pancreatic masses},
