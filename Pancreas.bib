	number = {4},
	journal = {Медицинская Визуализация},
	author = {Араблинский, А. В.},
	year = {2010},
	note = {Цитируемость в РИНЦ: 1},
	keywords = {{CT} {AND} {MRI}, {DIAGNOSTIC}, {PANCREAS} {MASS}, ДИАГНОСТИКА, КТ И МРТ, ОПУХОЛЬ ПОДЖЕЛУДОЧНОЙ ЖЕЛЕЗЫ},
	pages = {13--24},
