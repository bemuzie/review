	journal = {Gastrointestinal endoscopy},
	author = {Horwhat, John David and Paulson, Erik K and {McGrath}, Kevin and Branch, M Stanley and Baillie, John and Tyler, Douglas and Pappas, Theodore and Enns, Robert and Robuck, Gail and Stiffler, Helen and Jowell, Paul},
