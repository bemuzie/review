	pages = {583--590},
	file = {Mehmet Erturk et al. - 2006 - Pancreatic adenocarcinoma MDCT versus MRI in the .pdf:/home/denest/zoterolib/storage/XKSAWX34/Mehmet Erturk et al. - 2006 - Pancreatic adenocarcinoma MDCT versus MRI in the .pdf:application/pdf}
