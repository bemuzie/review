	keywords = {Adult, Aged, Algorithms, Case-Control Studies, Confounding Factors (Epidemiology), Cost of Illness, Disease Progression, Female, Follow-Up Studies, Humans, Male, Middle Aged, Pancreatic Neoplasms, United States},
