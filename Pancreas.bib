	pages = {1151--1158},
	file = {Izuishi et al. - 2010 - Impact of 18-fluorodeoxyglucose positron emission .pdf:/home/denest/zoterolib/storage/DQXGF7WD/Izuishi et al. - 2010 - Impact of 18-fluorodeoxyglucose positron emission .pdf:application/pdf}
