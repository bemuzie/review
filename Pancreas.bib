	number = {2},
	journal = {Pancreatology: official journal of the International Association of Pancreatology ({IAP)} ... [et al.]},
	author = {Klauss, M and Mohr, A and von Tengg-Kobligk, H and Friess, H and Singer, R and Seidensticker, P and Kauczor, H U and Richter, G M and Kauffmann, G W and Grenacher, L},
