	keywords = {Adenocarcinoma, Adult, Aged, Diagnosis, Differential, Female, Humans, Lymphoma, Large B-Cell, Diffuse, Lymphoproliferative Disorders, Male, Middle Aged, Neoplasm Grading, Pancreatic Neoplasms, Sarcoma, Myeloid},
	pages = {414--422}
}

@article{hayano_correlation_2013,
	title = {Correlation of apparent diffusion coefficient measured by diffusion-weighted {MRI} and clinicopathologic features in pancreatic cancer patients},
