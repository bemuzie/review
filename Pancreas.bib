	keywords = {Biopsy, Fine-Needle, Equipment Design, Granuloma, Plasma Cell, Humans, Needles, Pancreas, Pancreatic Diseases, Pancreatic Neoplasms, Predictive Value of Tests, Prospective Studies, Sensitivity and Specificity, Ultrasonography, Interventional},
	pages = {384--390}
