{PMCID:} {PMC2361120}},
	keywords = {Adult, Aged, Endosonography, Female, Humans, Laparoscopy, Male, Middle Aged, Neoplasm Invasiveness, Neoplasm Staging, Pancreatic Neoplasms, Tomography, X-Ray Computed},
	pages = {213--217}
