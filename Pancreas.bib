	number = {4},
	journal = {Journal of computer assisted tomography},
	author = {Mehmet Erturk, Sukru and Ichikawa, Tomoaki and Sou, Hironobu and Saitou, Ryo and Tsukamoto, Tatsuaki and Motosugi, Utarou and Araki, Tsutomu},
