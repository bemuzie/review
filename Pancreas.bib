	keywords = {Adult, Aged, Female, Humans, Imaging, Three-Dimensional, Male, Middle Aged, Neoplasm Invasiveness, Pancreatic Neoplasms, Portal Vein, Reproducibility of Results, Sensitivity and Specificity, Vascular Neoplasms},
