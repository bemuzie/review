	journal = {The American journal of surgical pathology},
	author = {Scudiere, Jennifer R and Shi, Chanjuan and Hruban, Ralph H and Herman, Joseph M and Fishman, Elliot K and Schulick, Richard D and Wolfgang, Christopher L and Makary, Martin A and Thornton, Katherine and Montgomery, Elizabeth and Horton, Karen M},
	month = apr,
	year = {2010},
	note = {{PMID:} 20351487 
{PMCID:} {PMC2861335}},
	keywords = {Adenocarcinoma, Adolescent, Adult, Aged, Combined Modality Therapy, Diagnosis, Differential, Female, Glucocorticoids, Humans, Male, Mesentery, Middle Aged, Pancreatectomy, Pancreatic Neoplasms, Panniculitis, Peritoneal, Sclerosis, Tamoxifen, Tomography, X-Ray Computed, Treatment Outcome},
