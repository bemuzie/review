	year = {1998},
	note = {{PMID:} 9627339},
	keywords = {Blood Glucose, Chronic Disease, Diabetes Complications, Fluorodeoxyglucose F18, Humans, Pancreatic Neoplasms, Pancreatitis, Radiopharmaceuticals, {ROC} Curve, Sensitivity and Specificity, Tomography, Emission-Computed},
	pages = {1030--1033},
	file = {Diederichs et al. - 1998 - FDG PET elevated plasma glucose reduces both upta.pdf:/home/denest/zoterolib/storage/PAMU9JDR/Diederichs et al. - 1998 - FDG PET elevated plasma glucose reduces both upta.pdf:application/pdf}
}

