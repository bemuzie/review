	volume = {12},
	copyright = {(c) 2011 The Authors. Journal of Digestive Diseases (c) 2011 Chinese Medical Association Shanghai Branch, Chinese Society of Gastroenterology, Renji Hospital  Affiliated to Shanghai Jiaotong University School of Medicine and Blackwell Publishing Asia Pty Ltd.},
