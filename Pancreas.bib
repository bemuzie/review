	note = {{PMID:} 18595144 
{PMCID:} {PMC2719240}},
	keywords = {Adenocarcinoma, Adult, Aged, {CA-19-9} Antigen, Female, Humans, Male, Middle Aged, Pancreatectomy, Pancreatic Neoplasms, Pancreaticoduodenectomy, Patient Selection, Predictive Value of Tests, Retrospective Studies, {ROC} Curve, Sensitivity and Specificity, Up-Regulation},
	pages = {3750--3753}
}

@article{malaguarnera_elevated_2009,
	title = {Elevated chromogranin A ({CgA)} serum levels in the patients with advanced pancreatic cancer},
	volume = {48},
	issn = {1872-6976},
	doi = {10.1016/j.archger.2008.01.014},
	abstract = {The neuroendocrine differentiation in {PC} could potentially represent a new finding with diagnostic, prognostic and therapeutic implications. This study aimed at evaluating the clinical usefulness of {CgA} as a neuroendocrine ({NE)} serum-marker. We investigated the role of the serum concentration of {CgA} in a study group of patients with {PC.} {CgA} was significantly higher in the patients affected by {PC} as compared with the group of healthy subjects ({HS)} and those with chronic pancreatitis ({CHP)} (p{\textless}0.001). Also the {HS} group differed significantly from the {CHP} control group in the serum {CgA} levels (p{\textless}0.001). The serum carbohydrate antigen ({CA19-9)} level displayed a significant difference (p{\textless}0.001) between the {PC} and the {HS} group. The {PC} and {CHP} groups, as well as the {HS} and {CHP} groups showed also significant differences in the {CA19-9} levels (p{\textless}0.001). One can conclude that the patients with higher {CgA} levels had poorer prognosis and survival, as compared to those with lower {CgA} levels. These results support the notion that the determination of serum {CgA} level before treatment may be a potential prognostic factor for {PC.}},
	language = {eng},
	number = {2},
	journal = {Archives of gerontology and geriatrics},
	author = {Malaguarnera, Michele and Cristaldi, Erika and Cammalleri, Lisa and Colonna, Valentina and Lipari, Helga and Capici, Alessandra and Cavallaro, Andrea and Beretta, Massimiliano and Alessandria, Innocenza and Luca, Salvatore and Motta, Massimo},
	month = apr,
